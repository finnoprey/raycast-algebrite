{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "algebrite",
  "title": "Algebrite",
  "description": "Implements the algebrite computer algebra system into Raycast.\n\nhttp://algebrite.org/",
  "icon": "command-icon.png",
  "author": "finnoprey",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Calculate",
      "description": "Runs an algebrite calculation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.5",
    "@raycast/utils": "^1.4.8",
    "algebrite": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
